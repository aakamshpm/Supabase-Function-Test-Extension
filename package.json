{
  "name": "supabase-function-tester",
  "displayName": "Supabase Function Tester",
  "description": "Test Supabase client functions directly in VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Testing",
    "Other"
  ],
  "activationEvents": [
    "onCommand:supabase-tester.openPanel"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "supabase-tester.openPanel",
        "title": "Test Supabase Function",
        "category": "Supabase"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0"
  }
}
